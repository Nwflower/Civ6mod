		<Replace Tag="LOC_CIVILIZATION_CIV_DAAIM_NAME" Language="zh_Hans_CN">
			<Text>天地一家大爱盟</Text>
		</Replace>
		<Replace Tag="LOC_CIVILIZATION_CIV_DAAIM_DESCRIPTION" Language="zh_Hans_CN">
			<Text>天地一家大爱盟</Text>
		</Replace>
		<Replace Tag="LOC_CIVILIZATION_CIV_DAAIM_ADJECTIVE" Language="zh_Hans_CN">
